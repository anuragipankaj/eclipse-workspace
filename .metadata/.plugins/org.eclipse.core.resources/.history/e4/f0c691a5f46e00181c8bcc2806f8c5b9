  {
	mainPage page= new mainPage(driver);
	Assert.assertTrue(page.verifyTitle(), "Home page title doesn't match");
	System.out.println("SUCCESS");
  }
@Test
 public void verifyLogin()
 {
	System.out.println("Current page :"+driver.getTitle());
	
 }


}
